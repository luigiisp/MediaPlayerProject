<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufrn.imd.controller.LoginScreenController">
   <children>
      <Button fx:id="loginButton" layoutX="174.0" layoutY="242.0" mnemonicParsing="false" onAction="#onLoginButtonPressed" text="Login" />
      <TextField fx:id="usernameTextField" layoutX="126.0" layoutY="111.0" promptText="username" />
      <TextField fx:id="passwordTextField" layoutX="126.0" layoutY="173.0" promptText="password" />
      <Label fx:id="loginReturnLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="8.0" layoutY="63.0" prefHeight="37.0" prefWidth="386.0" textAlignment="CENTER" textFill="RED" />
   </children>
</AnchorPane>
