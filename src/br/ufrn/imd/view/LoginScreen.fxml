<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufrn.imd.controller.gui.LoginScreenController">
	<children>
		<Button fx:id="loginButton" layoutX="175.0" layoutY="225.0"
			mnemonicParsing="false" onAction="#onLoginButtonPressed" text="Login" />
		<TextField fx:id="usernameTextField" layoutX="126.0"
			layoutY="111.0" promptText="username" />
		<TextField fx:id="passwordTextField" layoutX="126.0"
			layoutY="173.0" promptText="password" />
		<Label fx:id="loginReturnLabel" alignment="CENTER"
			contentDisplay="CENTER" layoutX="8.0" layoutY="63.0"
			prefHeight="37.0" prefWidth="386.0" textAlignment="CENTER"
			textFill="RED" />
		<Button fx:id="registerButton" layoutX="91.0" layoutY="268.0"
			mnemonicParsing="false" onAction="#onRegisterButtonPressed"
			text="Don't you have an account? Click here." />
	</children>
</AnchorPane>
