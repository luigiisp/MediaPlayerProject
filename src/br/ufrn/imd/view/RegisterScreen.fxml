<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufrn.imd.controller.gui.RegisterScreenController">
   <children>
      <TextField fx:id="fullNameTextField" layoutX="126.0" layoutY="66.0" promptText="Full name" />
      <TextField fx:id="usernameTextField" layoutX="126.0" layoutY="116.0" promptText="Username" />
      <TextField fx:id="passwordTextField" layoutX="126.0" layoutY="163.0" promptText="Password" />
      <Button fx:id="registerButton" layoutX="170.0" layoutY="237.0" mnemonicParsing="false" onAction="#onRegisterButtonPressed" text="Register" />
      <Label fx:id="registerReturnLabel" alignment="CENTER" layoutX="1.0" layoutY="30.0" prefHeight="17.0" prefWidth="400.0" />
      <Button fx:id="returnLoginButton" layoutX="132.0" layoutY="280.0" mnemonicParsing="false" onAction="#onReturnLoginButtonButtonPressed" text=" Return to login screen" />
      <CheckBox fx:id="checkBoxVip" layoutX="170.0" layoutY="201.0" mnemonicParsing="false" text="VIP" />
   </children>
</AnchorPane>
